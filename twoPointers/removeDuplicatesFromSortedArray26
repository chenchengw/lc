    public int removeDuplicates(int[] nums) {
        //note set both i, j to 1 since the first element is always there
        int i = 1, j = 1;
        while (i < nums.length) {
            if (nums[i] != nums[i-1]) {
                nums[j++] = nums[i];
            } 
            i++;
        }
        return j;
    }
